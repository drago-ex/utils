{
	"name": "drago-ex/utils",
	"type": "library",
	"description": "Extended ArrayHash with additional methods for converting data to arrays, including support for uppercase keys.",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Zdeněk Papučík",
			"homepage": "https://github.com/accgit"
		}
	],
	"require": {
		"php": ">=8.3 <9",
		"nette/utils": "^4.0"
	},
	"require-dev": {
		"nette/bootstrap": "^",
		"nette/tester": "^",
		"tracy/tracy": "^2.10",
		"phpstan/phpstan-nette": "^1.2.9"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"scripts": {
		"phpstan": "phpstan analyse --level 5 --configuration tests/phpstan.neon src",
		"tester": "tester tests -s",
		"tester-win": "tester -c tests/php.ini tests -s"
	}
}
